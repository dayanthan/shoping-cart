<%- model_class = Profile -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:address) %></th>
      <th><%= model_class.human_attribute_name(:city) %></th>
      <th><%= model_class.human_attribute_name(:state) %></th>
      <th><%= model_class.human_attribute_name(:country) %></th>
      <th><%= model_class.human_attribute_name(:mobile) %></th>
      <th><%= model_class.human_attribute_name(:image) %></th>
      <th><%= model_class.human_attribute_name(:date_of_birth) %></th>
     <!--  <th><%=t '.actions', :default => t("helpers.actions") %></th> -->
    </tr>
  </thead>
  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
<!--         <td><%#= link_to profile.id, profile_path(profile) %></td>
 -->    <%user=User.find_by_user_id(profile.user_id)%>
        <b><td><%= link_to user.user_name, profile_path(profile)%></td></b>
        <td><%= profile.address %></td>
        <td><%= profile.city %></td>
        <td><%= profile.state %></td>
        <td><%= profile.country %></td>
        <td><%= profile.mobile %></td>
        <td><%= image_tag profile.avatar.url(:thumb) %></td>
        <td><%= profile.date_of_birth %></td>
        <!-- <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_profile_path(profile), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      profile_path(profile),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td> -->
      </tr>
    <% end %>
  </tbody>
</table>

